<%= form_for(@report) do |f| %>
  	<%= f.text_field :title, placeholder: "Report title..." %><br/>
  	<%= f.text_field :project_id, placeholder: @project.id, type: "hidden" %>
    <%= f.text_area :content, placeholder: "Test result..." %><br/>
  	<%= f.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>